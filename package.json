{
  "name": "unified-intake-tool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages": "npm run pages:build && npm run pages:copy",
    "pages:build": "next build",
    "pages:copy": "mkdir -p _site && rm -rf _site/* && cp -r public/* _site/ && cp -r out/* _site/",
    "prettier": "npm prettier . --write",
    "prepare": "husky && husky install"
  },
  "dependencies": {
    "@trussworks/react-uswds": "^9.1.0",
    "next": "15.1.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix",
    "*.{js,css,md,tx,jsx,tsx,css}": "prettier --write"
  }
}
